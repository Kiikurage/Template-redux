{
	"compileOnSave": false,
	"compilerOptions": {
		"strict": true,
		"lib": [
			"dom",
			"es2015",
			"es2016",
			"es2017",
			"es2018"
		],
		"jsx": "react",
		"target": "es2016",
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"removeComments": false,
		"preserveConstEnums": true,
		"sourceMap": false,
		"skipLibCheck": true
	},
	"exclude": [
		"node_modules"
	]
}
